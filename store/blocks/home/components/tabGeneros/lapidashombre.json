{

  "flex-layout.row#lapidas-hombre": {
    "title": "Container lapidas",
    "children": [
      "stack-layout#zapatos-hombre",
      "stack-layout#pantalones-hombre",
      "stack-layout#camisas-hombre"
    ],
    "props": {
      "blockClass": "container-lapidas",
      "preserveLayoutOnMobile":true,
      "colSizing": "auto",
      "fullWidth": true
    }
  },
  "stack-layout#zapatos-hombre": {
    "title": "Container stack",
    "children": ["image#zapatos-hombre", "link#zapatos-hombre"],
    "props": {
      "blockClass":"container-stack"
    }
  },
  "image#zapatos-hombre": {
    "props": {
      "src": "assets/zapatos-hombre.png",
      "blockClass":"image-lapida"
    }
  },
  "link#zapatos-hombre": {
    "props": {
      "href": "#",
      "label": "ZAPATOS"
    }
  },
  "stack-layout#pantalones-hombre": {
     "title": "Container stack",
    "children": ["image#pantalones-hombre", "link#pantalones-hombre"],
    "props": {
      "blockClass":"container-stack"
    }
  },
  "image#pantalones-hombre": {
    "props": {
      "src": "assets/pantalones-hombre.png",
      "blockClass":"image-lapida"
    }
  },
  "link#pantalones-hombre": {
    "props": {
      "href": "#",
      "label": "PANTALONES"
    }
  },
  "stack-layout#camisas-hombre": {
     "title": "Container stack",
    "children": ["image#camisas-hombre", "link#camisas-hombre"],
    "props": {
      "blockClass":"container-stack"
    }
  },
  "image#camisas-hombre": {
    "props": {
      "src": "assets/camisas-hombre.png",
      "blockClass":"image-lapida"
    }
  },
  "link#camisas-hombre": {
    "props": {
      "href": "#",
      "label": "CAMISAS"
    }
  }

}
